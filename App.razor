<Router AppAssembly="@typeof(App).Assembly">
    <Found Context="routeData">
    
        <head>
          <title>Welcome to Lyrid</title>
          <meta name="description" content="Generated by create next app" />
          <link rel="icon" href="/favicon.ico" />
        </head>
        
        <div class="main">
          <div class="left">
            <div class="atas">
              <img src="/assets/img/logo.png" alt='logo'/>
              <br/>
              <h1 class="title">Welcome to Lyrid</h1>
              <p class="subtitle">Get started by editing your Blazer Web Assembly Application.
                <br/>
                <a href="https://nextjs.org/docs/getting-started">Documentation</a>
              </p>
            </div>
            <div>
              <img src="/assets/img/white.png" alt='mascot putih'/>
            </div>
          </div>
          <div class="right">
            <div class="docsEx">
              <a href="https://docs.lyrid.io" class="boxRight">
                <div>
                  <div class="boxTitle">
                    <h5 class="docs">Documentation</h5>
                    <img class="arrow" src="/assets/img/right-arrow.png" width="20" height="20" alt='right-arrow'/>
                  </div>
                  <p class="ex">Find in-depth information about Lyrid features and API.</p>
                </div>
              </a>
              <a href="https://github.com/LyridInc" class="boxRight">
                <div>
                  <div class="boxTitle">
                    <h5 class="docs">Examples</h5>
                    <img class="arrow" src="/assets/img/right-arrow.png" width="20" height="20" alt='right-arrow'/>
                  </div>
                  <p class="ex">Discover and deploy boilerplate example DOT.Net projects.</p>
                </div>
              </a>
            </div>
          </div>
        </div>      
    </Found>
    <NotFound>
        <PageTitle>Not found</PageTitle>
        <LayoutView Layout="@typeof(MainLayout)">
            <p role="alert">Sorry, there's nothing at this address.</p>
        </LayoutView>
    </NotFound>
</Router>
